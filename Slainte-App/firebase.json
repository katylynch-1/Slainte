{
  "hosting": {
    "site": "slainte-1",
    "source": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId":"slainte-app",
          "region":"europe-west1"
        }
      }
    ]
  }
}
